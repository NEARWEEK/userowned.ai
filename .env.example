# UserOwned.ai Environment Configuration

# Environment
NODE_ENV=development # development, staging, production

# Logging
LOG_LEVEL=info # debug, info, warn, error

# GitHub API
YOUR_GITHUB_TOKEN=your_github_token

# Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=-1001559796949 # POOOL group

# X (Twitter) via Zapier
ZAPIER_WEBHOOK_URL=your_zapier_webhook_url

# Feature Flags
ENABLE_DRY_RUN=false

# Optional APIs
DUNE_API_KEY=your_dune_api_key
DAPPRADAR_API_KEY=your_dappradar_key

# Mailchimp
MAILCHIMP_API_KEY=your_mailchimp_api_key
MAILCHIMP_SERVER=us21 # Check your API key suffix (e.g., us1, us21, etc.)
MAILCHIMP_LIST_ID=your_list_id

# Multimedia APIs
FIGMA_API_KEY=your_figma_personal_access_token
RUNWAY_API_KEY=your_runway_api_key
